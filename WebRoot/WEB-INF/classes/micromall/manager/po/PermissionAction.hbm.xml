<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="micromall.manager.vo.PermissionAction" table="tb_priv_sys_per_act">
		<id name="perActionId" type="java.lang.Integer">
			<column name="per_act_id" />
			<generator class="identity"></generator>
		</id>
		<property name="perId" type="java.lang.Integer">
			<column name="per_id" />
		</property>
		<property name="perMenuCode" type="java.lang.String">
			<column name="per_menu_code" />
		</property>
		<property name="perActionCode" type="java.lang.String">
			<column length="10" name="per_act_code" />
		</property>
	</class>
</hibernate-mapping>
