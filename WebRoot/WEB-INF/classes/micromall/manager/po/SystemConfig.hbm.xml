<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="micromall.manager.vo.SystemConfig" table="tb_priv_para_config">
		<id name="scid" type="java.lang.Integer">
			<column name="scid" />
			<generator class="identity"></generator>
		</id>
		<property name="key" type="java.lang.String">
			<column name="para_name" length="100" />
		</property>
		<property name="flag" type="java.lang.String">
			<column name="para_flag" length="10" />
		</property>
		<property name="value" type="java.lang.String">
			<column name="para_value" length="50" />
		</property>
		<property name="defaultValue" type="java.lang.String">
			<column name="default_value" length="50" />
		</property>
		<property name="des" type="java.lang.String">
			<column name="des" length="300" />
		</property>
	</class>
</hibernate-mapping>
